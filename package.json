{
  "name": "user-service",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "npm run migration:run && ts-node-dev ./src/index.ts",
    "start": "npm run migration:run && node ./build/index.js",
    "build": "tsc",
    "test": "jest --coverage --coverageReporters='text-summary'",
    "seed": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d ./src/database/data-source.ts",
    "seed:usuarios": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d ./src/database/data-source.ts --name src/database/seeds/user.seeder.ts",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/database/data-source.ts migration:generate ./src/database/migrations/default",
    "migration:run": "typeorm-ts-node-commonjs -d ./src/database/data-source.ts migration:run",
    "migration:revert": "typeorm-ts-node-commonjs -d ./src/database/data-source.ts migration:revert"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-prom-bundle": "7.0.0",
    "http-errors": "^2.0.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.14.1",
    "prom-client": "^15.1.3",
    "redoc-express": "2.1.0",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1",
    "typeorm": "^0.3.22",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.14.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "4.1.6",
    "ts-node-dev": "^2.0.0",
    "typeorm-extension": "^3.7.0",
    "typescript": "^5.8.3"
  }
}
